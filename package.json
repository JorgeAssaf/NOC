    "prisma": "5.7.0",
    "rimraf": "5.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3",
    "vitest": "1.0.2"
  },
  "dependencies": {
    "@prisma/client": "5.7.0",
    "@types/nodemailer": "6.4.14",
    "cron": "3.1.6",
    "dotenv": "16.3.1",
    "env-var": "7.4.1",
    "mongoose": "8.0.3",
    "nodemailer": "6.9.7"
